<?xml version="1.0"?>
<Container version="2">
  <Name>iDrive</Name>
  <Repository>tooliload/unraid-idrive</Repository>
  <Registry>https://hub.docker.com/r/tooliload/unraid-idrive</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category>Backup: Cloud:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/tooliload/docker-templates/master/templates/unraid-idrive.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/tooliload/docker-templates/master/templates/icons/idrive.pngg</Icon>
  <ExtraParams></ExtraParams>
  <PostArgs/>
  <CPUset>0</CPUset>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="BACKUPDIR" Target="/home/backup" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/</Config>
  <Config Name="DOCKERDIR" Target="/work/IDriveForLinux/idriveIt/" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/idrive-docker/idriveIt/</Config>
  <Config Name="iDrive Depencies" Target="/work/IDriveForLinux/scripts/Idrivelib/" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/idrive-docker/depencies/</Config>
</Container>
